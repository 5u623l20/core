<model>
  <mount>//Elastic/Filebeat</mount>
  <description>Filebeat plugin</description>
  <items>
    <general>
      <output type="OptionField">
        <Required>Y</Required>
        <default>logstash</default>
        <OptionValues>
          <logstash>Logstash</logstash>
        </OptionValues>
      </output>
    </general>
    <inputs>
      <log>
        <enabled type="OptionField">
          <Required>Y</Required>
          <default>true</default>
          <OptionValues>
            <true>True</true>
            <false>False</false>
          </OptionValues>
        </enabled>
        <paths type="TextField">
          <default>/var/log/suricata/eve.json</default>
        </paths>
        <encoding type="OptionField">
          <Required>Y</Required>
          <default>plain</default>
          <OptionValues>
            <plain>plain</plain>
            <utf-8>utf-8</utf-8>
            <utf-16be-bom>utf-16be-bom</utf-16be-bom>
            <utf-16be>utf-16be</utf-16be>
            <utf-16le>utf-16le</utf-16le>
            <big5>big5</big5>
            <gb18030>gb18030</gb18030>
            <gbk>gbk</gbk>
            <hz-gb-2312>hz-gb-2312</hz-gb-2312>
            <euc-kr>euc-kr</euc-kr>
            <euc-jp>euc-jp</euc-jp>
            <iso-2022-jp>iso-2022-jp</iso-2022-jp>
            <shift-jis>shift-jis</shift-jis>
          </OptionValues>
        </encoding>
        <exclude_lines type="TextField">
          <default></default>
        </exclude_lines>
        <include_lines type="TextField">
          <default></default>
        </include_lines>
        <exclude_files type="TextField">
          <default></default>
        </exclude_files>
        <harvester_buffer_size type="IntegerField">
          <default></default>
        </harvester_buffer_size>
        <max_bytes type="IntegerField">
          <default></default>
        </max_bytes>
      </log>
    </inputs>
    <outputs>
      <logstash>
        <hosts type="TextField">
          <default>localhost:5044</default>
        </hosts>
        <workers type="IntegerField">
          <default>0</default>
        </workers>
        <compression_level type="IntegerField">
          <default>0</default>
        </compression_level>
        <ttl type="IntegerField">
          <default>0</default>
        </ttl>
        <loadbalance type="OptionField">
          <Required>Y</Required>
          <default>false</default>
          <OptionValues>
            <true>True</true>
            <false>False</false>
          </OptionValues>
        </loadbalance>
        <pipelining type="IntegerField">
          <default></default>
        </pipelining>
        <slow_start type="OptionField">
          <Required>Y</Required>
          <default>false</default>
          <OptionValues>
            <true>True</true>
            <false>False</false>
          </OptionValues>
        </slow_start>
        <index type="TextField">
          <default></default>
        </index>
        <ssl>
          <enabled type="BooleanField">
            <Required>Y</Required>
            <default>0</default>
          </enabled>
          <ca type="TextField">
            <default></default>
          </ca>
          <client type="TextField">
            <default></default>
          </client>
          <key type="TextField">
            <default></default>
          </key>
        </ssl>
      </logstash>
    </outputs>
  </items>
</model>